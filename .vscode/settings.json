{
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "deno.enable": true,
  "deno.enablePaths": [
    "./supabase/functions/*"
  ],
  "deno.importMap": "./supabase/functions/import_map.json",
  "cSpell.words": [
    "Edit",
    "hookform",
    "lucide",
    "sonner",
    "stocktake",
    "supabase",
    "tanstack"
  ]
}